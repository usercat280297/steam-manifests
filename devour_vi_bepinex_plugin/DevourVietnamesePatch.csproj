<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <AssemblyName>DevourVietnamesePatch</AssemblyName>
    <RootNamespace>DevourVietnamesePatch</RootNamespace>
    <Version>1.0.0</Version>
    <Description>Vietnamese localization patch for DEVOUR game using BepInEx</Description>
    <Authors>Vietnamese Community</Authors>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <!-- BepInEx base - includes harmony -->
    <PackageReference Include="BepInEx.Core" Version="5.4.21" />
    <PackageReference Include="UnityEngine.Modules.CoreModule" Version="2020.3.35" />
  </ItemGroup>

</Project>
